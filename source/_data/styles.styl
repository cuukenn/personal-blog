/* sidebar social icon */
.fa-gitee {
  background-image: url('/images/icons/gitee.svg');
  background-size: 1em 1em;
  opacity: 0.55;
  background-position: 0.05rem 0.2rem;
  background-repeat: no-repeat;
  height: 1rem;
  width: 1rem; 
  border-radius: 0rem;
  &:hover {
      opacity: 1;
    }
}
